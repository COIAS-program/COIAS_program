{
	"name": "COIAS_program_github",
	"context": "..",
	"dockerComposeFile": "../../docker-compose.dev.yml",
	"workspaceFolder": "/opt/COIAS_program_github",
	"service": "back-dev-app",
	"settings": {},
	"extensions": ["ms-python.python","ms-azuretools.vscode-docker"],
	"onCreateCommand": "bash /opt/COIAS_program_github/Docker/setup.sh", //bind後にbuildなどを実行
	"updateContentCommand": "apt install -y bash-completion", //vscodeで接続時にだけtab補完がほしかったので
	"postCreateCommand": "echo \". /etc/bash_completion\" >> ~/.bashrc"
}
